<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Squareface.Windows">

	<ResourceDictionary.ThemeDictionaries>
		<ResourceDictionary x:Key="Default">
			<x:String x:Key="BackButtonGlyph">&#xE071;</x:String>
			<x:String x:Key="BackButtonSnappedGlyph">&#xE0BA;</x:String>

			<SolidColorBrush x:Key="RootsoupPageBackgroundThemeBrush" Color="#E6E6E6"/>
			<SolidColorBrush x:Key="RootsoupPageForegroundThemeBrush" Color="#055B5C"/>
		</ResourceDictionary>

		<ResourceDictionary x:Key="Experimental">
			<x:String x:Key="BackButtonGlyph">&#xE0A6;</x:String>
			<x:String x:Key="BackButtonSnappedGlyph">&#xE0C4;</x:String>

			<SolidColorBrush x:Key="RootsoupPageBackgroundThemeBrush" Color="#FFFDE8"/>
			<SolidColorBrush x:Key="RootsoupPageForegroundThemeBrush" Color="#0B64C0"/>
		</ResourceDictionary>

		<ResourceDictionary x:Key="Sunflower">
			<SolidColorBrush x:Key="RootsoupPagePrimaryThemeBrush" Color="#FFCE54"/>
			<SolidColorBrush x:Key="RootsoupPageSecondaryThemeBrush" Color="#F6BB42"/>
			<SolidColorBrush x:Key="RootsoupPageForegroundThemeBrush" Color="#28292B"/>
			<SolidColorBrush x:Key="RootsoupPageBackgroundThemeBrush" Color="#FAFCFE"/>
		</ResourceDictionary>

	</ResourceDictionary.ThemeDictionaries>

	
    
</ResourceDictionary>
